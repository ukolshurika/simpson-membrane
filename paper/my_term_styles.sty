\usepackage{hyperref}
%say NO to awful red boxes!
\hypersetup{
    colorlinks,
    citecolor=blue,
    filecolor=grey,
    linkcolor=red,
    urlcolor=blue
}


\definecolor{myred}{rgb}{0.6,0,0} % for strings
\definecolor{mygreen}{rgb}{0.25,0.5,0.35} % comments
\definecolor{mypurple}{rgb}{0.5,0,0.35} % keywords
\definecolor{mydocblue}{rgb}{0.25,0.35,0.75} % javadoc

%set pretty code view 
\lstset{
basicstyle=\ttfamily,
keywordstyle=\color{mypurple}\bfseries,
stringstyle=\color{myred},
commentstyle=\color{mygreen},
morecomment=[s][\color{mydocblue}]{/**}{*/},
numbers=left,
numberstyle=\tiny\color{black},
stepnumber=1,
numbersep=10pt,
tabsize=4,
showspaces=false,
showstringspaces=false}

 \graphicspath{{images/}}