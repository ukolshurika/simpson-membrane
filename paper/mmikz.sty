\newenvironment{newthm}[2][s]
{\vskip1mm \ifthenelse{\equal{#1}{s}}{{\small \sc #2.}}{{\small \sc #2 #1.}}}{}
\newenvironment{proof}[1][s]
{\vskip1mm \ifthenelse{\equal{#1}{s}}{{\it \langu{Proof. }{Доказательство. }}}{{\it #1. }}}{$\square$}
\newenvironment{theorem}[1][s]
{\vskip1mm \ifthenelse{\equal{#1}{s}}{{\small \sc \langu{Theorem. }{Теорема. }}}
{{\small \sc \langu{Theorem #1.}{Теорема #1.}}}}{}
\newenvironment{remark}[1][s]
{\vskip1mm \ifthenelse{\equal{#1}{s}}{{\small \sc \langu{Remark. }{Замечание. }}}
{{\small \sc \langu{Remark #1.}{Замечание #1.}}}}{}
\newenvironment{example}[1][s]
{\vskip1mm \ifthenelse{\equal{#1}{s}}{{\small \sc \langu{Example. }{Пример. }}}
{{\small \sc \langu{Example #1.}{Пример #1.}}}}{}
\newenvironment{lemma}[1][s]
{\vskip1mm \ifthenelse{\equal{#1}{s}}{{\small \sc \langu{Lemma. }{Лемма. }}}
{{\small \sc \langu{Lemma #1.}{Лемма #1.}}}}{}
\newenvironment{definition}[1][s]
{\vskip1mm \ifthenelse{\equal{#1}{s}}{{\small \sc \langu{Definition. }{Определение. }}}
{{\small \sc \langu{Definition #1.}{Определение #1.}}}}{}



